{
  "name": "storap-lms",
  "version": "1.0.0",
  "description": "LMS Admin Dashboard",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.storap.lms",
    "productName": "LMS Application",
    "win": {
      "icon": "icon.png",
