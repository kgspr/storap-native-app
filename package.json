      "owner": "kgspr",
      "repo": "storap-native-app"
    }
  },
  "dependencies": {
    "electron": "^latest",
    "electron-updater": "^4.4.0"
  },
  "devDependencies": {
    "electron-builder": "^22.0.0"
  }
}
